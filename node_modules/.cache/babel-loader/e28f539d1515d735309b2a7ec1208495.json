UAAA,MAAA,GAAA,IAAA,CAAA,SAAA,CAAA,KAAA,aAAA,CAAA;AACA,MAAA,YAAA,CAAA,OAAA,CAAA,eAAA,EAAA,MAAA;AACA,KALA;AAOA,IAAA,QAPA,sBAOA;AAAA;;AACA,UAAA,UAAA,GAAA,KAAA,aAAA,CAAA,GAAA,CAAA,UAAA,OAAA,EAAA;AACA,eAAA,OAAA,CAAA,EAAA;AACA,OAFA,CAAA;AAIA,UAAA,YAAA,GAAA;AACA,gBAAA,KAAA,YAAA,CAAA,IADA;AAEA,kBAAA,KAAA,YAAA,CAAA,MAFA;AAGA,iBAAA,KAAA,YAAA,CAAA,KAHA;AAIA,mBAAA,KAAA,YAAA,CAAA,OAJA;AAKA,6BAAA,KAAA,UALA;AAMA,8BAAA,SANA;AAOA,+BAAA;AAPA,OAAA;AAUA,MAAA,KAAA,CAAA,IAAA,CAAA,oCAAA,EAAA,YAAA,EACA,IADA,CACA;AAAA,eAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,SAAA,CAAA;AAAA,OADA,EAEA,KAFA,CAEA,UAAA,GAAA;AAAA,eAAA,OAAA,CAAA,GAAA,CAAA,GAAA,CAAA;AAAA,OAFA;AAIA,MAAA,YAAA,CAAA,UAAA,CAAA,eAAA;AACA;AA3BA;AA7CA,CAAA","sourcesContent":["<template>\r\n    <div class=\"ShoppingCart\">\r\n        <HeaderShayna/>\r\n        <!-- Breadcrumb Section Begin -->\r\n        <div class=\"breacrumb-section\">\r\n        <div class=\"container\">\r\n            <div class=\"row\">\r\n                <div class=\"col-lg-12 text-left\">\r\n                    <div class=\"breadcrumb-text product-more\">\r\n                        <a href=\"./home.html\"><i class=\"fa fa-home\"></i> Home</a>\r\n                        <span>Shopping Cart</span>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n        </div>\r\n        <!-- Breadcrumb Section Begin -->\r\n\r\n    <!-- Shopping Cart Section Begin -->\r\n        <section class=\"shopping-cart spad\">\r\n        <div class=\"container\">\r\n            <div class=\"row\">\r\n                <div class=\"col-lg-8\">\r\n                    <div class=\"row\">\r\n                        <div class=\"col-lg-12\">\r\n                            <div class=\"cart-table\">\r\n                                <table>\r\n                                    <thead>\r\n                                        <tr>\r\n                                            <th>Image</th>\r\n                                            <th class=\"p-name text-center\">Product Name</th>\r\n                                            <th>Price</th>\r\n                                            <th>Action</th>\r\n                                        </tr>\r\n                                    </thead>\r\n                                    <tbody v-if=\"keranjangUser.length > 0 \">\r\n                                        <tr v-for=\"keranjang in keranjangUser\" :key=\"keranjang.id\">\r\n                                            <td class=\"cart-pic first-row\">\r\n                                                <img class=\"img-cart\" :src=\"keranjang.photo\" />\r\n                                            </td>\r\n                                            <td class=\"cart-title first-row text-center\">\r\n                                                <h5>{{keranjang.name}}</h5>\r\n                                            </td>\r\n                                            <td class=\"p-price first-row\">${{keranjang.price}}</td>\r\n                                            <td @click=\"removeItem(keranjangUser.index)\" class=\"delete-item\"><a href=\"#\"><i class=\"material-icons\">\r\n                                              close\r\n                                              </i></a></td>\r\n                                        </tr>\r\n                                    </tbody>\r\n                                    <tbody v-else>\r\n                                        <tr>\r\n                                            <td colspan=\"4\">Keranjang Anda Masih Kosong, Silahkan Belanja Kembali</td>\r\n                                        </tr>\r\n                                    </tbody>\r\n                                </table>\r\n                            </div>\r\n                        </div>\r\n                        <div class=\"col-lg-8 text-left\">\r\n                            <h4 class=\"mb-4\">\r\n                                Informasi Pembeli:\r\n                            </h4>\r\n                            <div class=\"user-checkout\">\r\n                                <form>\r\n                                    <div class=\"form-group\">\r\n                                        <label for=\"namaLengkap\">Nama lengkap</label>\r\n                                        <input v-model=\"customerInfo.name\" type=\"text\" class=\"form-control\" id=\"namaLengkap\" aria-describedby=\"namaHelp\" placeholder=\"Masukan Nama\">\r\n                                    </div>\r\n                                    <div class=\"form-group\">\r\n                                        <label for=\"namaLengkap\">Email Address</label>\r\n                                        <input v-model=\"customerInfo.email\" type=\"email\" class=\"form-control\" id=\"emailAddress\" aria-describedby=\"emailHelp\" placeholder=\"Masukan Email\">\r\n                                    </div>\r\n                                    <div class=\"form-group\">\r\n                                        <label for=\"namaLengkap\">No. HP</label>\r\n                                        <input v-model=\"customerInfo.number\" type=\"text\" class=\"form-control\" id=\"noHP\" aria-describedby=\"noHPHelp\" placeholder=\"Masukan No. HP\">\r\n                                    </div>\r\n                                    <div class=\"form-group\">\r\n                                        <label for=\"alamatLengkap\">Alamat Lengkap</label>\r\n                                        <textarea v-model=\"customerInfo.address\" class=\"form-control\" id=\"alamatLengkap\" rows=\"3\"></textarea>\r\n                                    </div>\r\n                                </form>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n                <div class=\"col-lg-4 text-left\">\r\n                    <div class=\"row\">\r\n                        <div class=\"col-lg-12\">\r\n                            <div class=\"proceed-checkout\">\r\n                                <ul>\r\n                                    <li class=\"subtotal\">ID Transaction <span>#SH12000</span></li>\r\n                                    <li class=\"subtotal mt-3\">Subtotal <span>${{totalHarga}}.00</span></li>\r\n                                    <li class=\"subtotal mt-3\">Pajak <span>10% ${{ditambahPajak}}</span></li>\r\n                                    <li class=\"subtotal mt-3\">Total Biaya <span>$ {{totalBiaya}}.00</span></li>\r\n                                    <li class=\"subtotal mt-3\">Bank Transfer <span>Mandiri</span></li>\r\n                                    <li class=\"subtotal mt-3\">No. Rekening <span>2208 1996 1403</span></li>\r\n                                    <li class=\"subtotal mt-3\">Nama Penerima <span>Shayna</span></li>\r\n                                </ul>\r\n                                <a @click=\"checkout()\" href=\"#\" class=\"proceed-btn\">I ALREADY PAID</a>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n        </section>\r\n        <!-- Shopping Cart Section End -->\r\n        <FooterShayna/>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport HeaderShayna from '@/components/HeaderShayna.vue';\r\nimport FooterShayna from '@/components/FooterShayna.vue';\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n    name: 'cart',\r\n    data(){\r\n        return{\r\n            keranjangUser:[],\r\n            customerInfo:{\r\n                name:'',\r\n                email:'',\r\n                number:'',\r\n                address:''\r\n            }\r\n        }\r\n    },\r\n\r\n    components:{\r\n        HeaderShayna,\r\n        FooterShayna\r\n    },\r\n\r\n     computed:{\r\n        totalHarga(){\r\n            return this.keranjangUser.reduce(function(items,data){\r\n                return items + data.price\r\n            }, 0)\r\n        },\r\n\r\n        ditambahPajak(){\r\n            return (this.totalHarga*10)/100\r\n        },\r\n\r\n        totalBiaya(){\r\n            return this.totalHarga + this.ditambahPajak\r\n        }\r\n    },\r\n\r\n     mounted(){\r\n         if (localStorage.getItem('keranjangUser')) {\r\n            try {\r\n                this.keranjangUser = JSON.parse(localStorage.getItem('keranjangUser'));\r\n            } catch(e) {\r\n                localStorage.removeItem('keranjangUser');\r\n            }\r\n        }\r\n    },\r\n\r\n    methods:{\r\n        removeItem(index){\r\n            this.keranjangUser.splice(index, 1)\r\n            const parsed = JSON.stringify(this.keranjangUser);\r\n            localStorage.setItem('keranjangUser', parsed);\r\n        },\r\n\r\n        checkout(){\r\n            let productIds =  this.keranjangUser.map(function(product){\r\n                return product.id\r\n            })\r\n\r\n            let checkoutData = {\r\n                'name': this.customerInfo.name,\r\n                'number': this.customerInfo.number,\r\n                'email': this.customerInfo.email,\r\n                'address': this.customerInfo.address,\r\n                'transaction_total': this.totalBiaya,\r\n                'transaction_status': 'PENDING',\r\n                'transaction_details': productIds,\r\n            }\r\n\r\n            axios.post('http://localhost:8000/api/checkout', checkoutData)\r\n                 .then(() => this.$router.push('success'))\r\n                 .catch(err=>console.log(err))\r\n            \r\n            localStorage.removeItem('keranjangUser');\r\n        }       \r\n    }\r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.img-cart {\r\n    width: 80px;\r\n    height: 100px;\r\n}\r\n</style>"],"sourceRoot":"src/views"}]}